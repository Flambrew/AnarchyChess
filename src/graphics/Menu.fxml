   fx:controller="src.graphics.MenuController">
   <children>
      <Rectangle arcHeight="50.0" arcWidth="50.0" width="1024.0" height="360.0" layoutX="128.0"
         layoutY="90.0" stroke="BLACK" strokeType="INSIDE" />
